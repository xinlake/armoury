    <color name="armouryNavigationBarColor">#121212</color>
    <color name="armouryButtonPressed">#009688</color>
    <color name="armouryCameraBlank">#121212</color>
    <color name="armouryOptionPanel80">#80000000</color>

    <style name="ArmouryPage" parent="android:Theme.Material">
        <item name="android:windowTranslucentNavigation">false</item>
